{
  "name": "toy-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "latest",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "null": "latest",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0"
  },
  "eslintConfig": {
    "rules": {
      "no-multiple-empty-lines": 1,
      "import/no-unresolved": "off",
      "import/order": "warn"
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": []
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.ts$",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "build:dev": "webpack serve --config e2e/webpack.config.js --mode development",
    "clean": "rimraf lib esm"
  },
  "module": "esm/index.js",
  "files": [
    "esm",
    "lib",
    "src"
  ],
  "dependencies": {
    "webpack-dev-server": "^3.11.2"
  }
}
